{
	"extends": "@react-native/typescript-config/tsconfig.json",
	"compilerOptions": {
		"types": ["jest", "node"],
		"paths": {
			"@/*": ["./src/*"]
		},
		"allowImportingTsExtensions": false,
		"module": "esnext"
	},
	"include": [
		"./src/**/*",
		"index.d.ts",
		".eslintrc.js",
		"jest-setup.ts",
		"jest.config.js",
		"babel.config.js",
		"index.js"
	]
}
